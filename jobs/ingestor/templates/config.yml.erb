---
port: <%= p("webhook.port") %>

github:
  webhook_secret_tokens: <%= p("webhook.github_secret_keys") %>

pubsub:
  project_name: <%= p("pubsub.project_name") %>
  topic: <%= p("pubsub.topic") %>
  private_key_path: /var/vcap/jobs/ingestor/config/pubsub_private_key

metrics:
  datadog_api_key: <%= p("metrics.datadog_api_key") %>
  sentry_dsn: <%= p("metrics.sentry_dsn") %>
  environment: <%= p("metrics.environment") %>
